{% if user is defined %}
<!-- Profile contact callout section -->
<section id="profile-contact">
    <div class="container">
        <div class="row">
            <div class="col-sm-4 wow bounceInLeft">
                <div class="profile-item">
                    <i class="fa fa-envelope-o"></i>
                    <h5><a href="mailto:{{ user.contact.email }}">{{ user.contact.email }}</a></h5>
                </div>
            </div>
            <div class="col-sm-4 wow bounceInUp">
                <div class="profile-item">
                    <i class="fa fa-phone"></i>
                    <h5>{{ user.contact.telephone | phone }}</h5>
                </div>
            </div>
            <div class="col-sm-4 wow bounceInRight">
                <div class="profile-item">
                    <i class="fa fa-map-marker"></i>
                    <h5>{{ user.contact.fullAddress }}</h5>
                </div>
            </div>
        </div><!-- .row -->
    </div><!-- .container -->
</section>
<!-- Profile contact callout section -->
{% endif %}